{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Dock",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/apps/server",
      "outFiles": ["${workspaceFolder}/packages/apps/server/**/*.js"]
    },
    {
      "name": "Attach to Dev Site",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/sites/localhost",
      "outFiles": ["${workspaceFolder}/packages/sites/localhost/**/*.js"]
    }
  ]
}